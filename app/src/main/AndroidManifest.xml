<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cs3398.sportsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.HomeActivity" />
        <activity android:name=".View.BracketActivity" />
        <activity android:name=".View.NewBracketActivity" />
        <activity android:name=".View.SearchActivity" />
        <activity android:name=".View.MapsActivity" />
        <activity android:name=".View.ProfileActivity" />
        <activity android:name=".View.FirstRoundActivity" />
        <activity android:name=".View.SecondRoundActivity" />
        <activity android:name=".View.CreateAccountActivity">
        </activity>
    </application>

</manifest>